      </geometry>
      <material name="${prefix}white" />
    </visual>
    <xacro:if value="${simple_collision_model}">
      <collision>
        <origin xyz="0 0 0.03657" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.07314" radius="0.085915"/>
        </geometry>
      </collision>
